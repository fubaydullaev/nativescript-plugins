    "nativescript-epub-folio": {
      "root": "packages/nativescript-epub-folio",
      "projectType": "library",
      "sourceRoot": "packages/nativescript-epub-folio",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/nativescript-epub-folio",
            "tsConfig": "packages/nativescript-epub-folio/tsconfig.json",
            "packageJson": "packages/nativescript-epub-folio/package.json",
            "main": "packages/nativescript-epub-folio/index.d.ts",
            "assets": [
              "packages/nativescript-epub-folio/*.md",
              "packages/nativescript-epub-folio/index.d.ts",
              "LICENSE",
              {
                "glob": "**/*",
                "input": "packages/nativescript-epub-folio/platforms/",
                "output": "./platforms/"
              }
            ]
          }
        },
        "build.all": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx run nativescript-epub-folio:build",
              "node tools/scripts/build-finish.ts nativescript-epub-folio"
            ],
            "parallel": false
          }
        },
        "focus": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx g @nativescript/plugin-tools:focus-packages nativescript-epub-folio"
            ],
            "parallel": false
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "defaultProject": "server-demo"
}
